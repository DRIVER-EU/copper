{
  "name": "@driver/cscop-dashboard",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --modern",
    "build": "vue-cli-service build --modern",
    "unlink": "yarn unlinkcs",
    "linkcs": "yarn link @csnext/cs-client && yarn link @csnext/cs-split-panel && yarn link @csnext/cs-map && yarn link @csnext/cs-timeline && yarn link @csnext/cs-core  && yarn link @csnext/cs-form && yarn link @csnext/cs-log",
    "unlinkcs": "yarn unlink @csnext/cs-core && yarn unlink @csnext/cs-client && yarn unlink @csnext/cs-split-panel && yarn unlink @csnext/cs-map && yarn unlink @csnext/cs-timeline &&  yarn unlink @csnext/cs-log",
    "dev": "yarn serve",
    "dev:client": "yarn dev",
    "clean": "del-cli dist lib node_modules",
    "docker:build": "docker build --build-arg VUE_APP_COPPER_LAYER_URL=http://localhost:3007 --build-arg VUE_APP_COPPER_LOG_URL=http://localhost:3007/logs --build-arg VUE_APP_COPPER_SOCKET_SERVER_URL=http://localhost:3007 -t copper-gui ."
  },
  "dependencies": {
    "@csnext/cs-client": "^0.0.124",
    "@csnext/cs-core": "^0.0.124",
    "@csnext/cs-form": "^0.0.124",
    "@csnext/cs-log": "^0.0.124",
    "@csnext/cs-map": "^0.0.124",
    "@csnext/cs-split-panel": "^0.0.124",
    "@csnext/cs-timeline": "^0.0.124",
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "axios": "^0.19.0",
    "chroma-js": "^2.0.6",
    "class-transformer": "^0.2.2",
    "core-js": "^2.6.9",
    "date-fns": "^1.30.1",
    "geojson": "^0.5.0",
    "mapbox-gl": "^1.3.1",
    "source-map-support": "^0.5.13",
    "vis-timeline": "^5.1.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@csnext/vue-cli-plugin-cs": "^0.0.124",
    "@types/chroma-js": "^1.4.3",
    "@types/mapbox-gl": "^0.54.4",
    "@types/node": "^12.7.9",
    "@types/socket.io-client": "^1.4.32",
    "@types/vis": "^4.21.19",
    "@types/vue-markdown": "^2.2.1",
    "@vue/babel-preset-app": "^3.11.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-runtime": "^6.26.0",
    "dojo-typings": "^1.11.9",
    "fibers": "^4.0.1",
    "raw-loader": "^1.0.0",
    "sass": "^1.22.12",
    "typescript": "^3.6.3",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
