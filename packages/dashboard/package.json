{
  "name": "@driver/cscop-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --modern",
    "build": "vue-cli-service build --modern",
    "unlink": "yarn unlinkcs",
    "linkcs": "yarn link @csnext/cs-client && yarn link @csnext/cs-split-panel && yarn link @csnext/cs-map && yarn link @csnext/cs-timeline && yarn link @csnext/cs-core  && yarn link @csnext/cs-form && yarn link @csnext/cs-log",
    "unlinkcs": "yarn unlink @csnext/cs-core && yarn unlink @csnext/cs-client && yarn unlink @csnext/cs-split-panel && yarn unlink @csnext/cs-map && yarn unlink @csnext/cs-timeline &&  yarn unlink @csnext/cs-log",
    "dev": "yarn serve",
    "clean": "del-cli dist lib node_modules",
    "docker:build": "docker build --build-arg VUE_APP_COPPER_SERVER_PORT=3007 --build-arg VUE_APP_COPPER_LAYER_URL=http://localhost:3007 --build-arg VUE_APP_COPPER_LOG_URL=http://localhost:3007/logs --build-arg VUE_APP_COPPER_SOCKET_SERVER_URL=http://localhost:3007 -t copper-gui ."
  },
  "dependencies": {
    "@csnext/cs-client": "^0.0.115",
    "@csnext/cs-core": "^0.0.115",
    "@csnext/cs-split-panel": "^0.0.115",
    "@csnext/cs-timeline": "^0.0.115",
    "@csnext/cs-map": "^0.0.115",
    "@csnext/cs-form": "^0.0.115",
    "@csnext/cs-log": "^0.0.115",
    "@mapbox/mapbox-gl-draw": "^1.1.1",
    "class-transformer": "^0.2.2",
    "geojson": "^0.5.0",
    "axios": "~0.18.0",
    "chroma-js": "^2.0.3",
    "date-fns": "^1.30.1",
    "mapbox-gl": "^0.54.0",
    "source-map-support": "^0.5.11",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.14",
    "core-js": "^2.6.5",
    "timeline-plus": "https://github.com/rinzeb/timeline-plus/releases/download/v2.3.1-beta.1/timeline-plus-2.3.1.tgz",
    "handlebars": "^4.1.2",
    "simplebar-vue": "^0.1.6"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.32",
    "@csnext/vue-cli-plugin-cs": "^0.0.115",
    "@types/chroma-js": "^1.4.1",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/babel-preset-app": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "dojo-typings": "^1.11.9",
    "raw-loader": "^1.0.0",
    "typescript": "^3.4.5",
    "vue-template-compiler": "^2.6.10",
    "@types/node": "^11.12.2",
    "@types/vue-markdown": "^2.2.1",
    "@types/mapbox-gl": "^0.51.6",
    "babel-runtime": "^6.26.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
