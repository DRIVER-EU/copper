{
  "name": "@driver/cscop-dashboard",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --modern",
    "build": "vue-cli-service build --modern --mode development",
    "unlink": "yarn unlinkcs",
    "linkcs": "yarn link @csnext/cs-client && yarn link @csnext/cs-split-panel && yarn link @csnext/cs-map && yarn link @csnext/cs-timeline && yarn link @csnext/cs-core && yarn link @csnext/cs-form && yarn link @csnext/cs-log && yarn link @csnext/cs-markdown",
    "unlinkcs": "yarn unlink @csnext/cs-core && yarn unlink @csnext/cs-client && yarn unlink @csnext/cs-split-panel && yarn unlink @csnext/cs-map && yarn unlink @csnext/cs-timeline && yarn unlink @csnext/cs-log && yarn unlink @csnext/cs-markdown",
    "dev": "yarn serve",
    "dev:client": "yarn dev",
    "clean": "del-cli dist lib node_modules",
    "docker:build": "docker build --build-arg VUE_APP_COPPER_LAYER_URL=http://localhost:3007 --build-arg VUE_APP_COPPER_LOG_URL=http://localhost:3007/logs --build-arg VUE_APP_COPPER_SOCKET_SERVER_URL=http://localhost:3007 --build-arg CI_COMMIT_SHORT_SHA=$(date) -t drivereu/copper-gui:latest ."
  },
  "dependencies": {
    "@csnext/cs-client": "^0.0.128-beta.28",
    "@csnext/cs-core": "^0.0.128-beta.28",
    "@csnext/cs-form": "^0.0.128-beta.28",
    "@csnext/cs-log": "^0.0.128-beta.28",
    "@csnext/cs-map": "^0.0.128-beta.28",
    "@csnext/cs-markdown": "^0.0.128-beta.28",
    "@csnext/cs-split-panel": "^0.0.128-beta.28",
    "@csnext/cs-timeline": "^0.0.128-beta.28",
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "axios": "^0.19.2",
    "chroma-js": "^2.1.0",
    "class-transformer": "^0.2.3",
    "core-js": "^2.6.11",
    "date-fns": "^1.30.1",
    "geojson": "^0.5.0",
    "mapbox-gl": "^1.8.1",
    "source-map-support": "^0.5.16",
    "vis-timeline": "^5.1.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@csnext/vue-cli-plugin-cs": "^0.0.128-beta.28",
    "@types/chroma-js": "^1.4.3",
    "@types/mapbox-gl": "^1.8.1",
    "@types/node": "^12.12.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/vis": "^4.21.19",
    "@types/vue-markdown": "^2.2.1",
    "@vue/babel-preset-app": "^3.12.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-runtime": "^6.26.0",
    "dojo-typings": "^1.11.9",
    "fibers": "^4.0.2",
    "fork-ts-checker-webpack-plugin": "^2.0.0",
    "raw-loader": "^1.0.0",
    "sass": "^1.26.3",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "build": {
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  }
}
