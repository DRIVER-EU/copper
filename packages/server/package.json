    "@nestjs/websockets": "^6.7.1",
    "arangojs": "^6.11.1",
    "axios": "^0.19.0",
    "dotenv": "",
    "express": "^4.17.1",
    "kafka-node": "^4.1.3",
    "lockfile": "^1.0.4",
